    "@angular/localize": "19.0.5",
    "@angular/ssr": "19.0.6",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "@types/express": "5.0.0",
    "@types/node": "22.10.5",
    "express": "4.21.2",
    "ng-extract-i18n-merge": "2.13.1",
